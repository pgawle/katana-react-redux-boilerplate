{
  "parser": "babel-eslint",
  "parserOptions": {
    "babelOptions": {
      "configFile": "./config/babel.config.json"
    }
  },
  "plugins": ["prettier"],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": ["error"]
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
